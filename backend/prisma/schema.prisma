// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Enquete{
  id String @id @default(uuid())
  titulo String
  dataInicio DateTime
  dataFim DateTime
  opcoes String
  @@map("enquetes")
}